import (
	"fmt"
)

func main(){
fmt.Println("Substituir múltiplos de 3 por 'Fizz' e múltiplos de 5 por 'Buzz':")
	for i := 1; i <= 100; i++ {
		if i%3 == 0 && i%5 == 0 {
			fmt.Println("FizzBuzz")
		} else if i%3 == 0 {
			fmt.Println("Fizz")
		} else if i%5 == 0 {
			fmt.Println("Buzz")
		} else {
			fmt.Println(i)
		}
	}
  }
